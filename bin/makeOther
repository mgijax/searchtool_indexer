#!/bin/sh

cd `dirname $0`

INDEX_CONFIG=../searchtool_indexer.config

if [ ! -r ${INDEX_CONFIG} ]
then
    echo "Cannot read configuration file: ${INDEX_CONFIG}" | tee -a ${LOG}
    exit 1
fi
. ${INDEX_CONFIG}

###########################
# Make the Other Indexes
###########################

java -d64 -DCONFIG=${CONFIG_MASTER},${INDEX_CONFIG} -Xms4g -Xmx8g org.jax.mgi.index.index.MakeIndex /data/searchtool/build/otherExact/index/ o

java -d64 -DCONFIG=${CONFIG_MASTER},${INDEX_CONFIG} -Xms4g -Xmx8g org.jax.mgi.index.index.MakeIndex /data/searchtool/build/otherDisplay/index/ od
