#!/bin/sh

cd ..

INDEX_CONFIG=`pwd`/searchtool_indexer.config

if [ ! -r ${INDEX_CONFIG} ]
then
    echo "Cannot read configuration file: ${INDEX_CONFIG}" | tee -a ${LOG}
    exit 1
fi
. ${INDEX_CONFIG}

cd bin

# Create the Marker Related Indexes

#makeMarkers

# Create the vocab related Indexes

#makeVocab

# Create the Other Indexes

makeOther
